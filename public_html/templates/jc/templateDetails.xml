<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE install PUBLIC "-//Joomla! 2.5//DTD template 1.0//EN" "http://www.joomla.org/xml/dtd/2.5/template-install.dtd">
<extension version="3.0" type="template" client="site">
	<name>jc</name>
	<version>1.0</version>
	<creationDate>June 2016</creationDate>
	<author>JoomlaCommunity</author>
	<authorEmail>admin@joomla.org</authorEmail>
	<copyright>Copyright (C) 2005 - 2012 Open Source Matters, Inc. All rights reserved.</copyright>
	<description>Perfect template for the Dutch Joomla Community website</description>
	<files>
		<filename>component.php</filename>
		<filename>helper.php</filename>
		<filename>favicon.ico</filename>
		<filename>index.php</filename>
		<filename>templateDetails.xml</filename>
		<filename>template_preview.png</filename>
		<filename>template_thumbnail.png</filename>
		<folder>css</folder>
		<folder>html</folder>
		<folder>images</folder>
		<folder>img</folder>
		<folder>js</folder>
		<folder>language</folder>
		<folder>less</folder>
	</files>
	<positions>
		<position>copyright</position>
		<position>debug</position>
		<position>footer-1</position>
		<position>footer-2</position>
		<position>footer-3</position>
		<position>footer-4</position>
		<position>footer-5</position>
		<position>links</position>
		<position>mainmenu</position>
		<position>rechts</position>
		<position>slider</position>
		<position>submenu</position>
		<position>top-a</position>
		<position>usermenu</position>
	</positions>
	<languages folder="language">
		<language tag="en-GB">en-GB/en-GB.tpl_jc.ini</language>
		<language tag="en-GB">en-GB/en-GB.tpl_jc.sys.ini</language>
	</languages>
</extension>
